{
  "crons": [
    {
      "path": "/api/delete",
      "schedule": "0 20 * * *"
    }
  ],
  "functions": {
    "src/app/api/expo/files/route.js": {
      "runtime": "edge"
    },
    "src/app/api/expo/exist/route.js": {
      "runtime": "edge"
    },
    "src/app/api/unload/route.js": {
      "runtime": "edge"
    },
    "src/app/api/delete/route.js": {
      "runtime": "edge"
    }
  },
  "regions": ["bom1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/health"
    }
  ]
}
